<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
        </shape>
    </item>
    <item
        android:bottom="-100dp"
        android:left="-200dp"
        android:right="-200dp"
        android:top="0dp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/page_main" />

            <stroke
                android:color="@color/page_dividing_view"
                android:width="5dp"/>
        </shape>
    </item>
</layer-list>
